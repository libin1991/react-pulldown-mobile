{
  "name": "react-pulldown-mobile",
  "version": "2.0.6",
  "description": "一个简单的react下拉/上推组件",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && ./node_modules/.bin/babel src --copy-files --source-maps --extensions .es6,.es,.js,.jsx --out-dir dist && cp ./src/*.css ./dist/",
    "prepublish": "npm run build",
    "sample": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 5998",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/margox/react-pulldown-mobile.git"
  },
  "keywords": [
    "react",
    "pulldown",
    "pushup"
  ],
  "author": "Margox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/margox/react-pulldown-mobile/issues"
  },
  "homepage": "https://github.com/margox/react-pulldown-mobile#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "css-loader": "^0.25.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "html-webpack-plugin": "^2.17.0",
    "webpack-dev-server": "^1.14.1",
    "react-hot-loader": "^1.3.0",
    "webpack-hot-middleware": "^2.7.1"
  }
